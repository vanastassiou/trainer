{
  "glossary": {
    "categories": {
      "basic": "Fundamental weight training concepts",
      "anatomy": "Muscle and body part terminology",
      "movement": "Exercise movement patterns and mechanics",
      "programming": "Workout structure and planning terms",
      "technique": "Advanced training methods",
      "kinesiology": "Biomechanics and movement science terms",
      "direction": "Anatomical position and directional terminology"
    },
    "terms": [
      {
        "term": "repetition",
        "aliases": ["rep", "reps"],
        "category": "basic",
        "description": "One complete movement of an exercise, consisting of a concentric (lifting) and eccentric (lowering) phase.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "set",
        "aliases": [],
        "category": "basic",
        "description": "A group of consecutive repetitions performed without rest.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "one repetition maximum",
        "aliases": ["1RM", "one-rep max"],
        "category": "basic",
        "description": "The maximum weight that can be lifted for a single repetition with proper form. Used as a benchmark for calculating training loads.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Calculators/OneRepMax"
          }
        ]
      },
      {
        "term": "intensity",
        "aliases": [],
        "category": "basic",
        "description": "The amount of weight used, percentage of one repetition maximum, or the effort used during the exercise.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "volume",
        "aliases": [],
        "category": "basic",
        "description": "The total amount of work performed, typically calculated as sets × reps × weight.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Volume"
          }
        ]
      },
      {
        "term": "frequency",
        "aliases": [],
        "category": "basic",
        "description": "The number of workouts per week or number of times a muscle group is trained per unit of time.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "range of motion",
        "aliases": ["ROM"],
        "category": "basic",
        "description": "The degree of rotation available at a joint. Full range of motion in exercise means moving through the complete path of motion the joint allows.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Testing/JointROM"
          }
        ]
      },
      {
        "term": "tempo",
        "aliases": [],
        "category": "basic",
        "description": "The speed at which repetitions are performed, often expressed as a 4-point notation (e.g., 3/0/2/1) representing eccentric/stretched/concentric/contracted phases.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "warm-up set",
        "aliases": [],
        "category": "programming",
        "description": "A set performed with lighter resistance (typically 50% of working weight) for 12-15 reps before working sets to prepare muscles and joints for heavier loads.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/ExInfo/WarmUp"
          }
        ]
      },
      {
        "term": "working set",
        "aliases": ["work set"],
        "category": "programming",
        "description": "A set performed at the target training intensity, as opposed to warm-up or acclimation sets.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "compound exercise",
        "aliases": ["multi-joint exercise"],
        "category": "movement",
        "description": "An exercise that involves two or more simultaneous joint movements. Examples include squats, deadlifts, and bench press.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "isolation exercise",
        "aliases": ["single-joint exercise"],
        "category": "movement",
        "description": "An exercise that involves just one discernible joint movement. Examples include bicep curls, leg extensions, and lateral raises.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "basic exercise",
        "aliases": [],
        "category": "movement",
        "description": "A principal exercise that places greater absolute intensity on muscles exercised relative to auxiliary exercises. Typically compound movements.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "auxiliary exercise",
        "aliases": ["accessory exercise"],
        "category": "movement",
        "description": "An optional exercise that supplements a basic exercise, often placing greater relative intensity on a specific muscle or muscle head.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "closed-chain exercise",
        "aliases": [],
        "category": "movement",
        "description": "An exercise in which the end segment of the exercised limb is fixed or supporting the weight. Most compound exercises are closed-chain movements. Example: squat.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "open-chain exercise",
        "aliases": [],
        "category": "movement",
        "description": "An exercise in which the end segment of the exercised limb is not fixed or not supporting the weight. Example: leg extension.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "concentric",
        "aliases": ["concentric contraction", "positive"],
        "category": "movement",
        "description": "The phase of movement where the muscle shortens while generating force, typically the lifting or exertion phase.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "eccentric",
        "aliases": ["eccentric contraction", "negative"],
        "category": "movement",
        "description": "The phase of movement where the muscle lengthens while under tension, typically the lowering or return phase.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "isometric",
        "aliases": ["isometric contraction", "static hold"],
        "category": "movement",
        "description": "A muscle contraction where the muscle generates force without changing length. The joint angle remains constant.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "agonist",
        "aliases": ["prime mover"],
        "category": "anatomy",
        "description": "A muscle that causes motion by contracting. The primary muscle responsible for a movement.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "antagonist",
        "aliases": [],
        "category": "anatomy",
        "description": "A muscle that can move the joint opposite to the movement produced by the agonist. Relaxes or lengthens while the agonist contracts.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "synergist",
        "aliases": [],
        "category": "anatomy",
        "description": "A muscle that assists another muscle to accomplish a movement.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "stabilizer",
        "aliases": [],
        "category": "anatomy",
        "description": "A muscle that contracts with no significant movement to maintain a posture or fixate a joint during an exercise.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "dynamic stabilizer",
        "aliases": ["fixator"],
        "category": "anatomy",
        "description": "A biarticulate muscle that simultaneously moves through two joints with little change in length, shortening through the target joint while lengthening through an adjacent joint.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Questions/DynamicStabilizer"
          }
        ]
      },
      {
        "term": "flexion",
        "aliases": [],
        "category": "movement",
        "description": "Bending a joint, resulting in a decrease of angle between two body segments.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "extension",
        "aliases": [],
        "category": "movement",
        "description": "Straightening a joint, resulting in an increase of angle between two body segments.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "pronation",
        "aliases": [],
        "category": "movement",
        "description": "Rotation of the forearm so the palm faces downward, or inward rolling of the foot.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "supination",
        "aliases": [],
        "category": "movement",
        "description": "Rotation of the forearm so the palm faces upward, or outward rolling of the foot.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "abduction",
        "aliases": [],
        "category": "movement",
        "description": "Movement of a limb away from the midline of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "adduction",
        "aliases": [],
        "category": "movement",
        "description": "Movement of a limb toward the midline of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "hypertrophy",
        "aliases": ["muscle growth"],
        "category": "anatomy",
        "description": "The increase in muscle size resulting from training, primarily through enlargement of existing muscle fibers.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Volume"
          }
        ]
      },
      {
        "term": "atrophy",
        "aliases": ["muscle loss"],
        "category": "anatomy",
        "description": "The decrease in muscle size due to disuse, aging, or other factors.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "DOMS",
        "aliases": ["delayed onset muscle soreness"],
        "category": "anatomy",
        "description": "Muscle soreness that develops 24-48 hours after exercise, caused by microscopic injury to muscle fibers leading to inflammation. Most common after eccentric exercise or unfamiliar movements.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/ExInfo/Tidbits"
          }
        ]
      },
      {
        "term": "progressive overload",
        "aliases": [],
        "category": "programming",
        "description": "The gradual increase of stress placed on the body during training, achieved by increasing weight, reps, sets, or decreasing rest periods over time.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Guidelines"
          }
        ]
      },
      {
        "term": "failure",
        "aliases": ["muscular failure", "training to failure"],
        "category": "technique",
        "description": "The point at which no additional repetitions can be completed with proper form. Technical failure occurs when form breaks down.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Techniques"
          }
        ]
      },
      {
        "term": "superset",
        "aliases": [],
        "category": "technique",
        "description": "Performing two exercises back-to-back with minimal rest between them. Often used with opposing muscle groups (e.g., chest and back).",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Techniques"
          }
        ]
      },
      {
        "term": "drop set",
        "aliases": ["descending set", "strip set"],
        "category": "technique",
        "description": "A technique using progressively decreasing resistance after reaching failure. The exerciser reduces weight and continues reps with minimal rest.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Techniques"
          }
        ]
      },
      {
        "term": "forced repetitions",
        "aliases": ["assisted reps"],
        "category": "technique",
        "description": "Repetitions assisted by a training partner or spotter, typically performed after reaching failure to extend the set.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Techniques"
          }
        ]
      },
      {
        "term": "pre-exhaustion",
        "aliases": [],
        "category": "technique",
        "description": "Performing an isolation exercise immediately before a compound exercise for the same muscle group (e.g., chest fly before chest press).",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Techniques"
          }
        ]
      },
      {
        "term": "time under tension",
        "aliases": ["TUT"],
        "category": "technique",
        "description": "The total time a muscle is under load during a set. Manipulated by controlling tempo or increasing rep count.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "rate of perceived exertion",
        "aliases": ["RPE"],
        "category": "technique",
        "description": "A subjective scale (typically 1-10) measuring exercise intensity based on how hard the effort feels. RPE 10 means maximum effort with no reps left in reserve.",
        "references": []
      },
      {
        "term": "push",
        "aliases": ["pushing movement"],
        "category": "movement",
        "description": "A movement pattern where resistance is pushed away from the body. Includes exercises like bench press, overhead press, and push-ups.",
        "references": []
      },
      {
        "term": "pull",
        "aliases": ["pulling movement"],
        "category": "movement",
        "description": "A movement pattern where resistance is pulled toward the body. Includes exercises like rows, pull-ups, and curls.",
        "references": []
      },
      {
        "term": "hinge",
        "aliases": ["hip hinge"],
        "category": "movement",
        "description": "A movement pattern involving flexion and extension at the hip while maintaining a neutral spine. Key pattern in deadlifts, Romanian deadlifts, and good mornings.",
        "references": []
      },
      {
        "term": "squat pattern",
        "aliases": [],
        "category": "movement",
        "description": "A fundamental movement pattern involving simultaneous flexion at the hip, knee, and ankle while maintaining an upright torso.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Squats"
          }
        ]
      },
      {
        "term": "resistance curve",
        "aliases": [],
        "category": "movement",
        "description": "An exercise's characteristic change of force throughout its range of motion. May be ascending, descending, or bell-shaped.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "leg drive",
        "aliases": [],
        "category": "technique",
        "description": "Using the legs to generate force and stability during upper body exercises like bench press, achieved by driving feet firmly into the floor.",
        "references": []
      },
      {
        "term": "lockout",
        "aliases": [],
        "category": "movement",
        "description": "The end position of an exercise where the joints are fully extended. Example: top of a squat or bench press.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "sticking point",
        "aliases": [],
        "category": "movement",
        "description": "The most difficult portion of an exercise's range of motion, typically where leverage is least favorable.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "spotter",
        "aliases": [],
        "category": "basic",
        "description": "A training partner who provides safety assistance during heavy or challenging lifts, ready to help if the lifter cannot complete a rep.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/WeightTraining/Glossary"
          }
        ]
      },
      {
        "term": "form",
        "aliases": ["technique", "proper form"],
        "category": "basic",
        "description": "The correct body positioning and movement execution during an exercise to maximize effectiveness and minimize injury risk.",
        "references": []
      },
      {
        "term": "target",
        "aliases": ["target muscle"],
        "category": "anatomy",
        "description": "The primary muscle intended for exercise. The muscle group being specifically trained by a given movement.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "origin",
        "aliases": ["muscle origin"],
        "category": "anatomy",
        "description": "The muscle attachment point that moves least during contraction, generally more proximal (closer to the body's center).",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "insertion",
        "aliases": ["muscle insertion"],
        "category": "anatomy",
        "description": "The muscle attachment point that moves most during contraction, generally more distal (farther from the body's center).",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "biarticulate",
        "aliases": ["biarticular", "two-joint muscle"],
        "category": "anatomy",
        "description": "A muscle that crosses two joints and can produce movement at both. Examples include hamstrings, rectus femoris, and gastrocnemius.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "active insufficiency",
        "aliases": [],
        "category": "kinesiology",
        "description": "The inability of a biarticulate muscle to exert enough tension to shorten sufficiently to complete full range of motion in both joints simultaneously.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/ExInfo/Muscle"
          }
        ]
      },
      {
        "term": "passive insufficiency",
        "aliases": [],
        "category": "kinesiology",
        "description": "The inability of a biarticulate muscle to stretch enough to complete full range of motion in both joints simultaneously.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/ExInfo/Muscle"
          }
        ]
      },
      {
        "term": "isotonic",
        "aliases": ["isotonic contraction", "dynamic contraction"],
        "category": "movement",
        "description": "A muscle contraction with movement against resistance. Despite the name meaning 'same tension,' resistance varies through the range of motion due to changing leverage.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "isokinetic",
        "aliases": ["isokinetic contraction"],
        "category": "movement",
        "description": "A muscle contraction against force at a constant speed throughout the range of motion. Requires specialized equipment that adjusts resistance to match applied force.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "sagittal plane",
        "aliases": ["anterior-posterior plane"],
        "category": "direction",
        "description": "An anatomical plane that divides the body into left and right portions. Movements in this plane include flexion and extension.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "frontal plane",
        "aliases": ["coronal plane"],
        "category": "direction",
        "description": "An anatomical plane that divides the body into front and back portions. Movements in this plane include abduction and adduction.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "transverse plane",
        "aliases": ["horizontal plane", "xy plane"],
        "category": "direction",
        "description": "An anatomical plane that divides the body into upper and lower portions. Movements in this plane include rotation.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "anterior",
        "aliases": ["ventral"],
        "category": "direction",
        "description": "Toward the front of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "posterior",
        "aliases": ["dorsal"],
        "category": "direction",
        "description": "Toward the back of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "medial",
        "aliases": [],
        "category": "direction",
        "description": "Toward the midline (center) of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "lateral",
        "aliases": [],
        "category": "direction",
        "description": "Away from the midline (center) of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "proximal",
        "aliases": [],
        "category": "direction",
        "description": "Closer to the reference point or attachment to the trunk. For limbs, closer to the body's center.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "distal",
        "aliases": [],
        "category": "direction",
        "description": "Farther from the reference point or attachment to the trunk. For limbs, farther from the body's center.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "superior",
        "aliases": ["cranial"],
        "category": "direction",
        "description": "Toward the head or upper part of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
        ]
      },
      {
        "term": "inferior",
        "aliases": ["caudal"],
        "category": "direction",
        "description": "Away from the head or toward the lower part of the body.",
        "references": [
          {
            "source": "exrx",
            "url": "https://exrx.net/Kinesiology/Glossary"
          }
